{
	"name": "DidUpDesktop",
	"description": "A desktop app to access your school data based on didUP - Famiglia mobile",
	"keywords": [
		"electron",
		"react",
		"desktop-app",
		"typescript",
		"school"
	],
	"homepage": "https://github.com/DTrombett/didup-desktop#readme",
	"bugs": {
		"url": "https://github.com/DTrombett/didup-desktop/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/DTrombett/didup-desktop.git"
	},
	"license": "MIT",
	"author": "DTrombett",
	"main": "./src/main/main.ts",
	"scripts": {
		"build": "concurrently \"npm run build:main\" \"npm run build:renderer\"",
		"build:main": "cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.main.prod.ts",
		"build:renderer": "cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.renderer.prod.ts",
		"debug": "start \"DidUp Desktop\" \"./release/build/win-unpacked/DidUp Desktop.exe\"",
		"postinstall": "ts-node .erb/scripts/check-native-dep.js && electron-builder install-app-deps && cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.renderer.dev.dll.ts",
		"lint": "cross-env NODE_ENV=development eslint . --ext .js,.jsx,.ts,.tsx --fix",
		"package": "ts-node ./.erb/scripts/clean.js dist && npm run build && electron-builder build --publish never",
		"rebuild": "electron-rebuild --parallel --types prod,dev,optional --module-dir release/app",
		"start": "ts-node ./.erb/scripts/check-port-in-use.js && npm run start:renderer",
		"start:main": "cross-env NODE_ENV=development electronmon -r ts-node/register/transpile-only .",
		"start:preload": "cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.preload.dev.ts",
		"start:renderer": "cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack serve --config ./.erb/configs/webpack.config.renderer.dev.ts",
		"test": "eslint src && tsc --noEmit",
		"prepare": "npm test",
		"prettier": "prettier --write src/**/*.ts"
	},
	"browserslist": [],
	"permissions": [
		"openExternal"
	],
	"dependencies": {
		"bulma": "^0.9.4",
		"electron-debug": "^3.2.0",
		"electron-log": "^4.4.8",
		"electron-updater": "^5.3.0",
		"portaleargo-api": "github:dtrombett/portaleargo-api",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-router-dom": "^6.13.0",
		"source-map-support": "^0.5.21"
	},
	"devDependencies": {
		"@electron/notarize": "^1.2.4",
		"@electron/rebuild": "^3.2.10",
		"@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
		"@svgr/webpack": "^8.0.1",
		"@teamsupercell/typings-for-css-modules-loader": "^2.5.2",
		"@tsconfig/create-react-app": "^1.0.3",
		"@types/node": "20.3.1",
		"@types/react": "^18.2.12",
		"@types/react-dom": "^18.2.5",
		"@types/react-test-renderer": "^18.0.0",
		"@types/webpack-bundle-analyzer": "^4.6.0",
		"@typescript-eslint/eslint-plugin": "^5.59.11",
		"@typescript-eslint/parser": "^5.59.11",
		"browserslist-config-erb": "^0.0.3",
		"chalk": "^4.1.2",
		"concurrently": "^8.2.0",
		"cross-env": "^7.0.3",
		"css-loader": "^6.8.1",
		"css-minimizer-webpack-plugin": "^5.0.0",
		"detect-port": "^1.5.1",
		"electron": "^25.2.0",
		"electron-builder": "^24.4.0",
		"electron-devtools-installer": "^3.2.0",
		"electronmon": "^2.0.2",
		"eslint": "^8.42.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-erb": "^4.0.6",
		"eslint-import-resolver-typescript": "^3.5.3",
		"eslint-import-resolver-webpack": "^0.13.2",
		"eslint-plugin-compat": "^4.1.1",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-jsx-a11y": "^6.7.1",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-promise": "^6.1.1",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"file-loader": "^6.2.0",
		"html-webpack-plugin": "^5.5.3",
		"identity-obj-proxy": "^3.0.0",
		"mini-css-extract-plugin": "^2.7.6",
		"prettier": "^2.8.4",
		"react-refresh": "^0.14.0",
		"react-test-renderer": "^18.2.0",
		"sass": "^1.63.3",
		"sass-loader": "^13.3.2",
		"style-loader": "^3.3.3",
		"terser-webpack-plugin": "^5.3.9",
		"ts-loader": "^9.4.3",
		"ts-node": "^10.9.1",
		"tsconfig-paths-webpack-plugin": "^4.0.0",
		"typescript": "^5.1.3",
		"url-loader": "^4.1.1",
		"webpack": "^5.87.0",
		"webpack-bundle-analyzer": "^4.9.0",
		"webpack-cli": "^5.1.4",
		"webpack-dev-server": "^4.15.1",
		"webpack-merge": "^5.9.0"
	},
	"build": {
		"productName": "DidUp Desktop",
		"appId": "org.erb.didupdesktop",
		"asar": true,
		"asarUnpack": "**\\*.{node,dll}",
		"files": [
			"dist",
			"node_modules",
			"package.json"
		],
		"afterSign": ".erb/scripts/notarize.js",
		"mac": {
			"target": {
				"target": "default",
				"arch": [
					"arm64",
					"x64"
				]
			},
			"type": "distribution",
			"hardenedRuntime": true,
			"entitlements": "assets/entitlements.mac.plist",
			"entitlementsInherit": "assets/entitlements.mac.plist",
			"gatekeeperAssess": false
		},
		"dmg": {
			"contents": [
				{
					"x": 130,
					"y": 220
				},
				{
					"x": 410,
					"y": 220,
					"type": "link",
					"path": "/Applications"
				}
			]
		},
		"win": {
			"target": [
				"nsis"
			]
		},
		"linux": {
			"target": [
				"AppImage"
			],
			"category": "Development"
		},
		"directories": {
			"app": "release/app",
			"buildResources": "assets",
			"output": "release/build"
		},
		"extraResources": [
			"./assets/**"
		],
		"publish": {
			"provider": "github",
			"owner": "DTrombett",
			"repo": "DTrombett"
		}
	},
	"devEngines": {
		"node": ">=18.x",
		"npm": ">=8.x"
	},
	"engines": {
		"node": ">=18.x",
		"npm": ">=8.x"
	},
	"electronmon": {
		"patterns": [
			"!**/**",
			"src/main/**"
		],
		"logLevel": "quiet"
	}
}
